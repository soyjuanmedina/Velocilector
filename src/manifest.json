{
  "manifest_version": 3,
  "name": "Velocilector Extension",
  "description": "Aumenta tu velocidad de lectura",
  "version": "1.0.0",
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "icons": {
    "16": "assets/img/favicon.png",
    "128": "assets/img/favicon.png"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html"
  }
}